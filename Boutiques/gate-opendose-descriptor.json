{
    "command-line": "tar -zxf [GATERELEASE]; unzip [INDATA]; tar -zxvf gate_shared_libs.tar.gz; export G4ENSDFSTATEDATA=./G4ENSDFSTATE2.2; export G4LEDATA=./G4EMLOW7.3; ./Gate -a [Organ_ID,[ORGANID]][particle,[PARTICLETYPE]][energy,[ENERGY]][nb,[NBPRIMARIES]] [MACFILE] > output.log;  tar czf [RESULTS] ./output output.log", 

    "description": "tool description", 
    "error-codes": [
        {
            "code": 1, 
            "description": "Crashed"
        }
    ], 
    "inputs": [
        {
            "id": "outfilename", 
            "name": "name of the output tar.gz file", 
            "optional": false, 
            "type": "String", 
            "value-key": "[OUTFILENAME]"
        },
        {
            "id": "gaterelease", 
            "name": "LFN of the Gate Release used by the application", 
            "optional": false, 
            "type": "File", 
            "value-key": "[GATERELEASE]"
        },
        {
            "id": "indata", 
            "name": "LFN of the archive containing all input data", 
            "optional": false, 
            "type": "File", 
            "value-key": "[INDATA]"
        },
		{
            "id": "organid", 
            "name": "Organ ID ref to the organs table", 
            "optional": false, 
            "type": "String", 
            "value-key": "[ORGANID]"
        }, 
        {
            "id": "particletype", 
            "name": "Type of Particle to simulate", 
            "optional": false, 
            "type": "String", 
            "value-key": "[PARTICLETYPE]"
        }, 
        {
            "id": "energy", 
            "name": "The level of energy to simulate", 
            "optional": false, 
            "type": "String", 
            "value-key": "[ENERGY]"
        }, 
        {
            "id": "nbprimaries", 
            "name": "The number of primaries to simulate", 
            "optional": false, 
            "type": "Number", 
            "value-key": "[NBPRIMARIES]"
        }, 
        {
            "id": "macfile", 
            "name": "The name of the main macro file", 
            "optional": false, 
            "type": "String", 
            "value-key": "[MACFILE]"
        }
    ], 
	"output-files":[
		{
			"id" : "results",
			"name" : "results",
			"description" : "archive of the output folder containing execution results, and the output of the command",
			"path-template" : "[OUTFILENAME]",
			"optional": false,
			"value-key" : "[RESULTS]"
		}
	],
    "name": "GateCLforOpenDose", 
    "schema-version": "0.5", 
    "suggested-resources": {
        "cpu-cores": 1, 
        "ram": 1, 
        "walltime-estimate": 60
    }, 
    "tags": {
        "foo": "bar", 
        "purpose": "testing", 
        "status": "example"
    }, 
    "tool-version": "v0.1.0"
}